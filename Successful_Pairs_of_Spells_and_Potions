import java.util.Arrays;

class Solution {
    public int[] successfulPairs(int[] spells, int[] potions, long success) {
        int m=spells.length;
        int n=potions.length;
        Arrays.sort(potions);
        int maxportion=potions[n-1];
        int[] ans=new int[m];
        for(int i=0;i<m;i++){
            int spell=spells[i];
            long minportion=(long) Math.ceil((1.0*success)/spell);
            if(minportion>maxportion){
                ans[i]=0;
                continue;
            }
            int index=lowerB(potions,(int)minportion);
            ans[i]=n-index;
        }
        return ans;
    }
    private int lowerB(int []arr,int target){
        int l=0,r=arr.length;
        while(l<r){
            int mid=l+(r-l)/2;
            if(arr[mid]<target){
                l=mid+1;
            }
            else{
                r=mid;
            }
        }
        return l;
    }
} 
        
   
